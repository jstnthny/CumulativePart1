@model IEnumerable<CumulativeProjectPart1.Models.Teacher>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<nav style="overflow: hidden; margin-top: 5px;">
    <a class="navlink" href="/Teacher/New" style="float: left">New Teacher</a>
</nav>
<form action="/Teacher/List" method="get">
    <input type="text" name="SearchKey" />
    <input type="submit" value="Search" />

</form>

<p>This page should be showing information about all the teachers</p>

@foreach (var teacher in Model)
{

    <div><a href="/Teacher/Show/@teacher.TeacherId">@teacher.TeacherFname @teacher.TeacherLname</a></div>
}

